{
    "name": "levarion/moonwalker-api",
    "description": "Customized PHP Rest framework.",
    "keywords": ["levarion", "moonwalker-api", "micro", "rest", "restful", "api"],
    "license": "Custom",
    "type": "library",
    "authors": [
        {
            "name": "Paul S.",
            "email": "contact@winterei.se",
            "homepage": "https://winterei.se",
            "role": "Lead Code Monkey"
        }
    ],
    "require": {
        "league/route": "@stable",
        "league/booboo": "@stable",
        "zendframework/zend-diactoros": "@stable",
        "sabre/event": "^5",
        "monolog/monolog": "@stable",
        "vlucas/valitron": "@stable",
        "lcobucci/jwt": "@stable",
        "swiftmailer/swiftmailer": "@stable",
        "jmhobbs/swiftmailer-transport-aws-ses": "^0.9.2",
        "google/recaptcha": "~1.1",
        "adldap2/adldap2": "7.0.*",
        "php-http/guzzle6-adapter": "^1.1",
        "payum-tw/ecpay": "^1.1",
        "florianv/swap": "^3.1",
        "php-http/message": "^1.6",
        "geoip2/geoip2": "~2.0",
        "predis/predis": "^1.1",
        "maghead/maghead": "dev-master",
        "easypost/easypost-php": "@stable"
    },
    "require-dev": {
    },
    "bin": ["bin/phprest"],
    "autoload": {
        "psr-4": {
            "Moonwalker\\": "app",
            "Moonwalker\\Core\\": "core",
            "Moonwalker\\Schema\\": "app/Schema"
        }
    },
    "suggest": {
        "ext-msgpack": "Enables support for application/msgpack and application/x-msgpack handling",
        "ext-apcu": "Enables support for native data caching within PHP",
        "ext-igbinary": "Enables support for igbinary in event processing"
    },
    "minimum-stability" : "dev",
    "prefer-stable" : true
}
