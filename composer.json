{
    "name": "levarion/moonwalker-api",
    "description": "Customized PHP Rest framework.",
    "keywords": ["levarion", "moonwalker-api", "micro", "rest", "restful", "api"],
    "license": "Custom",
    "type": "library",
    "authors": [
        {
            "name": "Paul S.",
            "email": "contact@winterei.se",
            "homepage": "https://winterei.se",
            "role": "Lead Code Monkey"
        }
    ],
    "require": {
        "phprest/phprest": "dev-master",
        "phprest/phprest-service-validator": "dev-master",
        "phprest/phprest-service-request-filter": "dev-master",
        "maghead/maghead": "@stable",
        "swiftmailer/swiftmailer": "@stable",
        "jmhobbs/swiftmailer-transport-aws-ses": "^0.9.2",
        "google/recaptcha": "~1.1",
        "adldap2/adldap2": "7.0.*",
        "php-http/guzzle6-adapter": "^1.1",
        "payum-tw/ecpay": "^1.1",
        "florianv/swap": "^3.1",
        "php-http/message": "^1.6",
        "geoip2/geoip2": "~2.0",
        "predis/predis": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.2.5",
        "mockery/mockery": "~0.9.2",
        "squizlabs/php_codesniffer": "~2.3.0"
    },
    "bin": ["bin/phprest"],
    "autoload": {
        "psr-4": {
            "Phprest\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phprest\\Stub\\": "stub/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "suggest": {
        "phprest/phprest-service-request-filter": "For filtering Entites.",
        "phprest/phprest-service-validator": "For validating Entities.",
        "phprest/phprest-service-orm": "For managing Relational Databases."
    }
}
